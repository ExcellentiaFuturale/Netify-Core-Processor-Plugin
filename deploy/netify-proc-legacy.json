{
    "format": "json",
    "compressor": "none",
    "sinks": {
        "sink-http-legacy": {
            "default": {
                "enable": false,
                "type": "legacy-http",
                "format": "json",
                "compressor": "gz"
            }
        },
        "sink-socket": {
            "default": {
                "enable": true,
                "type": "legacy-socket"
            }
        },
        "sink-mqtt": {
            "flows": {
                "enable": false,
                "type": "stream-flows"
            },
            "stats": {
                "enable": false,
                "type": "stream-stats"
            }
        }
    }
}
