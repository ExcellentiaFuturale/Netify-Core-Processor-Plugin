{
    "format": "json",
    "compressor": "none",
    "flow-filters": [
      "proto == 'DHCP' or proto == 'DHCPv6';",
      "proto == 'HTTP' or proto == 'SSDP';"
    ],
    "sinks": {
        "sink-http": {
            "legacy": {
                "enable": false,
                "types": [ "legacy-http" ],
                "format": "json",
                "compressor": "gz"
            }
        },
        "sink-socket": {
            "default": {
                "enable": true,
                "types": [ "legacy-socket" ]
            },
            "websocket": {
                "enable": false,
                "types": [ "stream-flows", "stream-stats" ]
            }
        },
        "sink-mqtt": {
            "flows": {
                "enable": false,
                "types": [ "stream-flows" ]
            },
            "stats": {
                "enable": false,
                "types": [ "stream-stats" ]
            }
        }
    }
}
